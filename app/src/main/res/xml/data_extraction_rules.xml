<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="database" path="remote_checkin.db"/>
        <exclude domain="no-backup"/>
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="."/>
        <include domain="database" path="remote_checkin.db"/>
        <exclude domain="no-backup"/>
    </device-transfer>
</data-extraction-rules>
